{
  "name": "pac-man",
  "version": "1.0.0",
  "description": "Remake of the NES version for Pac-Man game",
  "scripts": {
    "clean": "rimraf .parcel-cache dist",
    "start:dev": "npm run clean && parcel serve ./index.html",
    "build": "npm run clean && parcel build ./index.html --dist-dir ./dist --public-url ./ --no-optimize --no-scope-hoist",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "author": "ymoreiratiti@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@excaliburjs/plugin-tiled": "^0.29.0",
    "excalibur": "^0.29.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "buffer": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "parcel": "2.12.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  }
}
